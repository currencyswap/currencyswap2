{
  "name": "Member",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "username": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "email": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "password": {
      "type": "string",
      "required": true
    },
    "fullName": {
      "type": "string"
    },
    "birthday": {
      "type": "date",
      "postgresql": {
        "dataType": "date",
        "nullable": "YES"
      }
    },
    "nationalId": {
      "type": "string"
    },
    "cellphone": {
      "type": "string"
    },
    "profession": {
      "type": "string"
    },
    "expiredDate": {
      "type": "date",
      "postgresql": {
        "dataType": "date",
        "nullable": "YES"
      }
    },
    "status": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "groups": {
      "type": "hasMany",
      "model": "Group",
      "foreignKey": "memberId",
      "keyThrough": "groupId",
      "through": "MemberGroup"
    },
    "addresses": {
      "type": "hasMany",
      "model": "Address",
      "foreignKey": "memberId"
    }
  },
  "acls": [],
  "methods": {}
}
