{
  "name": "Notification",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "string"
    },
    "memberId": {
      "type": "number",
      "required": true
    },
    "creatorId": {
      "type": "number"
    },
    "createdDate": {
      "type": "date",
      "required": true,
      "default": "now"
    },
    "isRead": {
      "type": "boolean",
      "required": true,
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "member": {
      "type": "belongsTo",
      "model": "Member",
      "foreignKey": "memberId"
    },
    "creator": {
      "type": "belongsTo",
      "model": "Member",
      "foreignKey": "creatorId"
    }
  },
  "acls": [],
  "methods": {}
}
