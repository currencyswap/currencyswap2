{
  "name": "Order",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "code": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "createdDate": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "updatedDate": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "closedDate": {
      "type": "date"
    },
    "rate": {
      "type": "number",
      "required": true
    },
    "give": {
      "type": "number",
      "required": true
    },
    "giveCurrencyId": {
      "type": "number",
      "required": true
    },
    "get": {
      "type": "number",
      "required": true
    },
    "getCurrencyId": {
      "type": "number",
      "required": true
    },
    "ownerId": {
      "type": "number",
      "required": true
    },
    "accepterId": {
      "type": "number"
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "Submitted"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "Member",
      "foreignKey": "ownerId"
    },
    "accepter": {
      "type": "belongsTo",
      "model": "Member",
      "foreignKey": "accepterId"
    },
    "giveCurrency": {
      "type": "belongsTo",
      "model": "Currency",
      "foreignKey": "giveCurrencyId"
    },
    "getCurrency": {
      "type": "belongsTo",
      "model": "Currency",
      "foreignKey": "getCurrencyId"
    },
    "activities": {
      "type": "hasMany",
      "model": "Activity",
      "foreignKey": "orderId",
      "keyThrough": "activityId",
      "through": "OrderActivity"
    }
  },
  "acls": [],
  "methods": {}
}
